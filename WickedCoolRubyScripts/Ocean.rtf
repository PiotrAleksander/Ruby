\par \pard\plain \s0\ql\widctlpar\ltrpar\sl276\slmult1{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\sb0\sa200\cf0\hich\af10\langfe1045\dbch\af11\afs22\lang1025\loch\f4\fs22\lang1045\qj\tx708\tx1416\sb0\sa0{\dbch\af9\afs20\rtlch \ltrch\loch\fs20\loch\f3
